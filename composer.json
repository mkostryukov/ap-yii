{
    "name": "mkostryukov/ap-yii",
    "description": "AP Yii Project",
    "keywords": ["ap"],
    "homepage": "http://www.allprojectors.ru",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/mkostryukov/ap-yii"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "bower-asset/admin-lte": "^2.3",
        "rmrevin/yii2-fontawesome": "~2.15",
        "bower-asset/jquery-slimscroll": "^1.3"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
